<UserControl x:Class="Kulami.HelpScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="920" d:DesignWidth="1760">
    <Grid>
        <Canvas Name="HelpBackground" HorizontalAlignment="Left" Height="920" VerticalAlignment="Top" Width="1760">
            <Button Name="BackButton" Content="" Canvas.Left="733" Canvas.Top="824" Width="66" Height="66" BorderBrush="{x:Null}" FontFamily="webdings" FontSize="92" HorizontalAlignment="Center" VerticalAlignment="Center" TextOptions.TextHintingMode="Animated" Foreground="#FFBEEAFF" Click="BackButtonClick" Style="{StaticResource MyButtonStyle}" Background="{x:Null}"/>
            <Button Name="NextButton" Content="" Canvas.Left="934" Canvas.Top="824" Width="66" Height="66" BorderBrush="{x:Null}" FontFamily="webdings" FontSize="92" HorizontalAlignment="Center" VerticalAlignment="Center" TextOptions.TextHintingMode="Animated" Foreground="#FFBEEAFF" Click="NextButtonClick" Style="{StaticResource MyButtonStyle}" Background="{x:Null}"/>
            <Button Name="MainButton" Content="main menu" Canvas.Left="1509" Canvas.Top="836" Width="226" Height="62" Background="#822B2B2B" BorderBrush="#FF015E8B" BorderThickness="7" FontFamily="SquareFont" FontSize="36" HorizontalAlignment="Center" VerticalAlignment="Center" TextOptions.TextHintingMode="Animated" Foreground="#FF4192B9" Click="Button_Click" Style="{StaticResource MyButtonStyle}"/>
        </Canvas>
    </Grid>
</UserControl>
